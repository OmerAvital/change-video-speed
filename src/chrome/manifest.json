{
  "name": "Change Video Speed",
  "description": "Change the speed of any video.",
  /* Added with WebPack */
  "version": null,
  "manifest_version": 3,
  "icons": {/* Added with WebPack */},
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "background",
    "webNavigation",
    "storage"
  ],
  "host_permissions": ["<all_urls>"]
}
